@{
    ViewBag.Title = "UploadFile";
}

<div class="container mt-4">
    <h4>Ingrese Archivo</h4>
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="exampleFormControlFile1">Ingresar archivo CSV para carga de datos</label>
        </div>
        <div class="form-group">
            <input type="file" class="form-control-file" name="file" required>
        </div>
        <div class="form-group">
            <input type="submit" value="Cargar" class="btn btn-primary btn btn-block" />
        </div>
    </form>
</div>

